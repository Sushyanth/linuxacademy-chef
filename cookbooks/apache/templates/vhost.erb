
<VirtualHost *: <%= @port%>>
	ServerName <%= @domain%>
	#ServerAlias <%= @domain%>5>

	DocumentRoot<%= @document_root%>

#StaticContent
	<Directory/>
		Options FollowSymLinks
		AllowOverride None
	</Directory>
	<Directory <%= @document_root%> >
		Options Indexes FollowSymLinks MultiViews
		AllowOverride None
		Order allow,deny
		allow from all
	</Directory>
</VirtualHost>
